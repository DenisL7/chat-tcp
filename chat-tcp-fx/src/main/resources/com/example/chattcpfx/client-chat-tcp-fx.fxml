<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="disconnectButton" alignment="CENTER" prefHeight="375.0" prefWidth="497.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chattcpfx.SentMessageController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TextArea fx:id="messagesTextArea" prefHeight="200.0" prefWidth="200.0" />
      <TextField fx:id="messageField" />
      <Button fx:id="sendMessageButton" mnemonicParsing="false" onAction="#handle" text="send" />
      <Button mnemonicParsing="false" onAction="#disconnect" text="disconnect" />
   </children>
</VBox>
